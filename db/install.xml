      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="idx_id_submission" UNIQUE="false" FIELDS="submission" COMMENT="Speed up looking for pages from a particular submission"/>
      </INDEXES>
